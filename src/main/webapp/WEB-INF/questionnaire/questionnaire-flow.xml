<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xsi:schemaLocation="http://www.springframework.org/schema/webflow 
							http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
	
	<view-state id="quote" view="quote" model="answers">
		<var name="answers" class="answers.Answers" />
    	<on-render>
    		<evaluate expression="riskDataQuestionnaire.getSomeQuestions" />
    	</on-render>
    	<transition on="submit" to="test" />
    	<transition on="Next" to="test" />
    </view-state>
	
	<view-state id="test" view="test" />
	
</flow>